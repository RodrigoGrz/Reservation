generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows", "linux-musl", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PropertyTypes {
  id String @id @default(uuid())
  type String
  created_at DateTime @default(now())

  @@map("property_types")
}

model Customer {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  created_at DateTime @default(now())

  @@map("customers")
}

model Hosting {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  created_at DateTime @default(now())

  @@map("hostings")
}